			"methods": {
				"addOwner(address)": {
					"params": {
						"newOwner": "is an address of a new owner."
					}
				},
				"approve(address,uint256)": {
					"details": "See {IERC20-approve}."
				},
				"constructor": {
					"details": "Sets name and symbol of token, gives initial amount of tokens to owners. For other parameters see {MyERC20Mint-constructor} and {MyOwnable-constructor}.",
					"params": {
						"name_": "sets name of token.",
						"symbol_": "sets symbol of token."
					}
				},
				"mint(address)": {
					"details": "Override {MyERC20Mint-mint} with `onlyOwner` modifier."
				},
				"prohibitMint()": {
					"details": "Override {MyERC20Mint-prohibitMint} with `onlyOwner` modifier."
				},
				"transfer(address,uint256)": {
					"details": "See {IERC20-transfer}."
				},
				"transferFrom(address,address,uint256)": {
					"details": "See {IERC20-transferFrom}."
				}
			},
			"title": "Custom ERC20 token.",
